# Doxyfile 1.9.3

# Project related configuration options
PROJECT_NAME           = "Jeu-De-La-Vie"
PROJECT_NUMBER         =
PROJECT_BRIEF          =
PROJECT_LOGO           =

# Output directory
OUTPUT_DIRECTORY       = ./doc
# Create a subdirectory for each version
CREATE_SUBDIRS         = NO

# Put comments after member function declarations
JAVADOC_AUTOBRIEF      = YES

# Set the input source code directory
INPUT                  = ./src
# Exclude the build directory
EXCLUDE                = ./target

# Optimize output for C++
OPTIMIZE_OUTPUT_FOR_C  = YES
# Extract all members from files
EXTRACT_ALL            = YES
# Put function and class documentation in the header
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES
EXTRACT_LOCAL_METHODS  = YES

# If the QUIET tag is set to YES, the messages that are generated by doxygen are
# suppressed. (NO by default)
QUIET                  = NO

# The RECURSIVE tag can be used to turn specify whether or not subdirectories
# should be searched for input files as well.
RECURSIVE              = YES

# The FILE_PATTERNS tag can be used to specify one or more wildcard pattern
# (like *.cpp *.h)
FILE_PATTERNS          = *.java

# The FILTER_PATTERNS tag can be used to specify filters on file patterns.
FILTER_PATTERNS        =

# If the EXAMPLES tag is set to YES, all examples in the sources are included.
# (NO by default)
EXAMPLES               = YES

# If the SORT_MEMBER_DOCS tag is set to YES, the member documentation will be
# sorted by member name. If set to NO, the documentation will appear in
# definition order.
SORT_MEMBER_DOCS       = YES

# If the SORT_BRIEF_DOCS tag is set to YES, the brief documentation will be
# sorted by member name. If set to NO, the documentation will appear in
# definition order.
SORT_BRIEF_DOCS        = YES

# The GENERATE_LATEX tag can be used to specify whether or not the LaTeX-related
# files should be generated.
GENERATE_LATEX         = NO

# Set the GENERATE_HTML tag to YES to generate HTML output
GENERATE_HTML          = YES

# The HTML_FILE_EXTENSION tag can be used to specify the file extension for
# each generated HTML page (default is .html)
HTML_FILE_EXTENSION    = .html

# Set the GENERATE_XML tag to YES to generate XML output
GENERATE_XML           = NO

# The XML_OUTPUT tag is used to specify where the XML pages will be put
XML_OUTPUT             = ./xml
